{
  "name": "asl-deployments",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "exit 0",
    "preconfigure": "mkdir -p deploy && rm -rf deploy/*",
    "configure": "kube-cookbook --config versions.yml --config ip-addresses.yml --out deploy",
    "configure:dev": "npm run configure -- --config dev/urls.yml dev/.kube.*.yml",
    "configure:preprod": "npm run configure -- --config preprod/urls.yml preprod/.kube.*.yml",
    "configure:prod": "npm run configure -- --config prod/urls.yml prod/.kube.*.yml",
    "test": "npm run test:lint && npm run test:dev && npm run test:preprod && npm run test:prod",
    "test:lint": "eslint tests",
    "test:dev": "npm run configure:dev -- --dry-run",
    "test:preprod": "npm run configure:preprod -- --dry-run",
    "test:prod": "npm run configure:prod -- --dry-run",
    "test:wait": "node ./lib/wait",
    "test:integration": "wdio run ./tests/wdio.conf.js",
    "test:visual": "wdio run ./tests/visual-regression/config.js",
    "test:audit": "audit-ci --high --skip-dev --config .auditrc.json",
    "test:check-wdio-log-file": "node ./lib/check-wdio-log-file/index.js",
    "docker:test": "docker run -e SELENIUM_HOST=host.docker.internal -v $PWD/tests:/app/tests -it asl-integration-tests",
    "docker:build": "docker build --platform=linux/amd64 --secret id=github_token,env=GITHUB_AUTH_TOKEN --secret id=token,env=ART_AUTH_TOKEN -t asl-integration-tests .",
    "predocker:test": "npm run docker:build",
    "autoproject": "tests/autoproject/bin/test.js",
    "autoproject:run": "wdio run",
    "shipit": "kd --context=acp-notprod_ASL --namespace=asl-dev -f deploy",
    "shipit:preprod": "kd --context=acp-notprod_ASL --namespace=asl-preprod -f deploy",
    "shipit:prod": "kd --context=acp-prod_ASL --namespace=asl-prod -f deploy",
    "seed:dev": "npm run configure:dev && kd --context=acp-notprod_ASL --namespace=asl-dev -f deploy/1-database-seed.job-deployment.yaml && kd --context=acp-notprod_ASL --namespace=asl-dev -f deploy/1-workflow-seed.job-deployment.yaml && kd --context=acp-notprod_ASL --namespace=asl-dev -f deploy/2-rebuild-search-index.job-deployment.yaml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/asl-deployments.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/asl-deployments/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/asl-deployments#readme",
  "devDependencies": {
    "@wdio/cli": "^8.32.3",
    "@wdio/local-runner": "^8.32.3",
    "@wdio/mocha-framework": "^8.27.0",
    "@wdio/reporter": "^8.32.2",
    "@wdio/runner": "^8.32.3",
    "@wdio/spec-reporter": "^8.32.2",
    "aws-sdk": "^2.1436.0",
    "chromedriver": "^121.0.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-config-lennym": "^2.3.2",
    "filenamify": "^6.0.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "moment": "^2.29.4",
    "moment-business-time": "^2.0.0",
    "resemblejs": "^5.0.0",
    "wdio-image-comparison-service": "^5.0.3"
  },
  "dependencies": {
    "@asl/kube-templates": "^5.2.4",
    "@cyber2024/pdf-parse-fixed": "^1.2.5",
    "@lennym/commit": "^1.0.1",
    "@lennym/drone-node": "^2.1.0",
    "@octokit/rest": "^20.0.1",
    "@ukhomeoffice/asl-constants": "^2.1.5",
    "@wdio/codemod": "^0.12.0",
    "canvas": "^2.11.2",
    "chalk": "^5.3.0",
    "csv-parse": "^5.4.0",
    "jimp": "^0.22.10",
    "jscodeshift": "^0.15.0",
    "mammoth": "^1.6.0",
    "mocha": "^10.2.0",
    "pdfjs-dist": "^3.9.179",
    "qrcode-reader": "^1.0.4",
    "r2": "^2.0.1"
  }
}
