{
  "name": "asl-internal-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon -r dotenv/config -e .js,.jsx",
    "test": "eslint .",
    "test:functional:ci": "asl-test ./tests/functional/config",
    "test:functional": "funkie --driver chromedriver npm run test:functional:ci",
    "build": "npm run build:js && npm run build:css",
    "build:js": "node --max_old_space_size=4096 node_modules/.bin/webpack",
    "prebuild:css": "mkdir -p ./public/css",
    "build:css": "npm-sass ./assets/sass/app.scss > ./public/css/app.css",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/asl-internal-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/asl-internal-ui/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/asl-internal-ui#readme",
  "dependencies": {
    "@asl/components": "^3.14.0",
    "@asl/pages": "^15.34.0",
    "@asl/projects": "^3.2.0",
    "@asl/service": "^7.2.2",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@ukhomeoffice/react-components": "^0.6.0",
    "archiver": "^3.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "body-parser": "^1.18.3",
    "docx": "^5.0.0-rc1",
    "express": "^4.16.4",
    "filenamify": "^4.0.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@ukhomeoffice/asl-functional-testing": "^1.2.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-lennym": "^2.0.1",
    "funkie": "0.0.6",
    "funkie-chromedriver": "0.0.1",
    "nodemon": "^1.18.9",
    "npm-sass": "^2.3.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  }
}
