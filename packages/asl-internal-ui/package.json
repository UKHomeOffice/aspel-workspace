{
  "name": "asl-internal-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon -r dotenv/config -e .js,.jsx",
    "test": "eslint . && npm run test:deduped",
    "test:deduped": "./tests/dedupe.sh",
    "test:functional:ci": "asl-test ./tests/functional/config",
    "test:functional": "funkie --driver chromedriver npm run test:functional:ci",
    "build": "npm run build:js && npm run build:css",
    "build:js": "node --max_old_space_size=4096 node_modules/.bin/webpack",
    "prebuild:css": "mkdir -p ./public/css",
    "build:css": "npm-sass ./assets/sass/app.scss > ./public/css/app.css",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/asl-internal-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/asl-internal-ui/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/asl-internal-ui#readme",
  "dependencies": {
    "@asl/components": "^7.2.3",
    "@asl/pages": "^18.23.4",
    "@asl/projects": "^6.4.2",
    "@asl/service": "^7.18.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@ukhomeoffice/react-components": "^0.8.6",
    "archiver": "^3.1.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "body-parser": "^1.19.0",
    "csv-stringify": "^5.3.6",
    "date-fns": "^1.30.1",
    "docx": "^5.0.0-rc4",
    "express": "^4.17.1",
    "filenamify": "^4.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "react": "^16.9.0",
    "react-redux": "^7.1.1",
    "uuid-validate": "0.0.3"
  },
  "devDependencies": {
    "@ukhomeoffice/asl-functional-testing": "^1.2.4",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-lennym": "^2.0.1",
    "funkie": "0.0.6",
    "funkie-chromedriver": "0.0.1",
    "nodemon": "^1.19.2",
    "npm-sass": "^2.3.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
