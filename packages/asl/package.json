{
  "name": "asl",
  "version": "1.1.0",
  "description": "Animal Science Licensing",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "npm-run-all --parallel watch:client watch:server watch:css",
    "watch:client": "npm run build:js -- --watch",
    "watch:server": "nodemon -r dotenv/config -e js,jsx,json -w",
    "watch:css": "npm run build:css -- --watch",
    "test": "npm run test:lint && npm run test:deduped",
    "test:deduped": "./test/dedupe.sh",
    "test:lint": "eslint . --ext jsx,js",
    "build": "npm run build:js && npm run build:css",
    "build:js": "node --max_old_space_size=4096 node_modules/.bin/webpack",
    "prebuild:css": "mkdir -p ./public/css",
    "build:css": "node-sass ./assets/sass/app.scss -o ./public/css --importer ./node_modules/npm-sass/lib/importer",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/asl.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/asl/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/asl#readme",
  "dependencies": {
    "@asl/components": "^10.14.2",
    "@asl/constants": "1.2",
    "@asl/pages": "^27.13.4",
    "@asl/projects": "^10.4.1",
    "@asl/service": "^9.0.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@ukhomeoffice/frontend-toolkit": "^2.13.1",
    "@ukhomeoffice/react-components": "^0.10.1",
    "body-parser": "^1.18.3",
    "deep-diff": "^1.0.2",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "r2": "^2.0.1",
    "react": "^16.9.0",
    "react-redux": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "dotenv": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-config-lennym": "^2.2.1",
    "eslint-plugin-undocumented-env": "^1.0.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "npm-sass": "^3.0.0",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
