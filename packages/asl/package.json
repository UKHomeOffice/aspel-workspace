{
  "name": "asl",
  "version": "1.1.0",
  "description": "Animal Science Licensing",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon -r dotenv/config -e js,jsx,json -w .",
    "test": "npm run test:lint",
    "test:lint": "eslint .",
    "build": "npm run build:js && npm run build:css",
    "build:js": "node --max_old_space_size=4096 node_modules/.bin/webpack",
    "prebuild:css": "mkdir -p ./public/css",
    "build:css": "npm-sass ./assets/sass/app.scss > ./public/css/app.css",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/asl.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/asl/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/asl#readme",
  "dependencies": {
    "@asl/components": "^3.12.0",
    "@asl/pages": "^15.24.1",
    "@asl/peephole": "^1.6.0",
    "@asl/service": "^7.0.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@ukhomeoffice/react-components": "^0.7.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "r2": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "dotenv": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-config-lennym": "^2.0.1",
    "eslint-plugin-undocumented-env": "^1.0.0",
    "nodemon": "^1.18.9",
    "npm-sass": "^2.3.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.1"
  }
}
