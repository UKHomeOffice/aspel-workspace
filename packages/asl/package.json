{
  "name": "asl",
  "version": "1.0.0",
  "description": "Animal Science Licensing",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon -r dotenv/config",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint . --ext jsx,js",
    "test:unit": "jest",
    "test:functional": "wdio ./tests/functional/config",
    "test:functional:local": "funkie --driver chromedriver npm run test:functional",
    "prebuild:css": "mkdir -p ./public/css",
    "build:css": "npm-sass ./assets/sass/app.scss > ./public/css/app.css",
    "prebuild:js": "mkdir -p ./public/js && mkdir -p ./assets/js/.pages",
    "build:js": "webpack",
    "postbuild:js": "rm -rf ./assets/js/.pages",
    "build": "npm run build:css && npm run build:js",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/asl.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/asl/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/asl#readme",
  "dependencies": {
    "@asl/dictionary": "^1.0.7",
    "@asl/pdf-renderer": "^1.0.0",
    "@asl/service": "^1.4.1",
    "@ukhomeoffice/asl-components": "^1.3.1",
    "csv-stringify": "^3.0.0",
    "express": "^4.16.3",
    "govuk-react-components": "^0.7.0",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "r2": "^2.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.3.0",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-lennym": "^2.0.0",
    "funkie": "0.0.6",
    "funkie-chromedriver": "0.0.1",
    "jest": "^22.4.3",
    "mocha": "^5.0.0",
    "nodemon": "^1.17.2",
    "npm-sass": "^2.2.1",
    "redux-logger": "^3.0.6",
    "string-replace-loader": "^2.1.1",
    "wdio-mocha-framework": "^0.5.12",
    "webdriverio": "^4.10.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12"
  }
}
