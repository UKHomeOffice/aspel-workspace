{
  "name": "asl",
  "version": "1.0.0",
  "description": "Animal Science Licensing",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "npm run test:lint",
    "test:lint": "eslint . --config lennym",
    "test:functional": "wdio ./tests/functional/config",
    "test:functional:local": "funkie --driver chromedriver npm run test:functional",
    "prebuild:css": "mkdir -p ./public/css",
    "build:css": "npm-sass ./assets/sass/app.scss > ./public/css/app.css",
    "prebuild:js": "mkdir -p ./public/js",
    "build:js": "webpack",
    "build": "npm run build:css && npm run build:js",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/asl.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/asl/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/asl#readme",
  "dependencies": {
    "@asl/dictionary": "^1.0.3",
    "@asl/service": "^1.3.1",
    "govuk-react-components": "^0.2.1",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "prop-types": "^15.6.1",
    "r2": "^2.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chalk": "^2.3.0",
    "eslint": "^4.15.0",
    "eslint-config-lennym": "^1.4.0",
    "funkie": "0.0.6",
    "funkie-chromedriver": "0.0.1",
    "mocha": "^5.0.0",
    "npm-sass": "^2.2.1",
    "wdio-mocha-framework": "^0.5.12",
    "webdriverio": "^4.10.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.12"
  }
}
