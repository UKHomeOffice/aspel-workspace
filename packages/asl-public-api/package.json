    "eslint": "^8.30.0",
    "@ukhomeoffice/eslint-config-asl": "^3.0.0",
    "eslint-plugin-undocumented-env": "^1.0.0",
    "knex": "^3.1.0",
    "mocha": "^11.1.0",
    "nodemon": "^3.1.9",
    "qs": "^6.5.2",
    "sinon": "^19.0.2",
    "supertest": "^3.4.2",
    "uuid": "^3.4.0"
  }
