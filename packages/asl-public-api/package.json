    "url": "https://github.com/UKHomeOffice/asl-public-api/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/asl-public-api#readme",
  "dependencies": {
    "@asl/schema": "^11.1.4",
    "@asl/service": "^11.0.2",
    "@ukhomeoffice/asl-constants": "^2.2.0",
    "express": "^4.18.2",
    "http-proxy": "^1.18.1",
    "into-stream": "^6.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "objection": "^3.1.5",
    "r2": "^2.0.1",
    "redis": "^4.5.1",
