{
  "presets": [
    [
      "@babel/preset-react",
      { "runtime": "automatic" }
    ],
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "targets": {
          "ie": 11
        },
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties"
  ]
}
