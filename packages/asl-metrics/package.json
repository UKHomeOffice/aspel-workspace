    "eslint": "^8.57.1",
    "mocha": "^10.7.3",
    "nodemon": "^2.0.4",
    "sinon": "^9.0.2",
    "uuid": "^8.1.0"
  },
  "overrides": {
    "@ukhomeoffice/frontend-toolkit": "^3.0.0"
  }
