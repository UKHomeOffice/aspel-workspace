  "dependencies": {
    "@asl/projects": "^15.7.0",
    "@asl/service": "^10.5.3",
    "@ukhomeoffice/asl-constants": "^2.2.0",
    "@ukhomeoffice/asl-dictionary": "^2.1.0",
    "express": "^4.17.1",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "moment-business-time": "^2.0.0",
